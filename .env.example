# Required Configuration
STORACHA_AGENT_DELEGATION=your-delegation-token      # Delegation token for Storacha access
STORACHA_AGENT_PRIVATE_KEY=your-private-key          # Agent's private key for signing operations

# Optional Configuration
GATEWAY=https://w3s.link                             # IPFS gateway URL (default: https://w3s.link)

# Required if your are using: https://www.npmjs.com/package/@storacha/client
STORACHA_SERVICE_DID=did:web:up.storacha.network
STORACHA_SERVICE_URL=https://up.storacha.network
STORACHA_STORE_NAME=storacha-cli
STORACHA_RECEIPTS_URL=https://up.storacha.network/receipt/

# Required if you are using: https://www.npmjs.com/package/@web3-storage/w3up-client
W3_SERVICE_DID=did:web:up.storacha.network
W3_STORE_NAME=storacha-cli
W3UP_SERVICE_URL=https://up.storacha.network
W3UP_RECEIPTS_ENDPOINT=https://up.storacha.network/receipt/

# Test Settings (only needed for running tests)
TEST_ROOM_ID=test-room-123                          # Test room identifier
TEST_AGENT_ID=test-agent-123                        # Test agent identifier
